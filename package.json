{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "fumadocs-mdx && next build",
    "dev": "next dev --turbo --port 3011",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "clean": "rm -rf .turbo node_modules .next",
    "typecheck": "next typegen && tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check . --write"
  },
  "dependencies": {
    "@vercel/analytics": "1.5.0",
    "class-variance-authority": "0.7.1",
    "fumadocs-core": "16.0.2",
    "fumadocs-mdx": "13.0.0",
    "fumadocs-openapi": "9.6.2",
    "fumadocs-ui": "16.0.2",
    "lucide-react": "0.546.0",
    "next": "16.0.0",
    "prettier": "3.6.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "remark": "15.0.1",
    "remark-gfm": "4.0.1",
    "remark-mdx": "3.1.1",
    "shiki": "3.13.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/postcss": "4.1.16",
    "@types/mdx": "2.0.13",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.16",
    "typescript": "5.9.3"
  }
}
